<%= form_for @link do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :p_id %><br />
    <%= f.text_field :p_id %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p><%= f.submit %></p>
<% end %>
