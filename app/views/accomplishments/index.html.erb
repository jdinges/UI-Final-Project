<% title "Accomplishments" %>

<table>
  <tr>
    <th>P</th>
    <th>Content</th>
  </tr>
  <% for accomplishment in @accomplishments %>
    <tr>
      <td><%= accomplishment.p_id %></td>
      <td><%= accomplishment.content %></td>
      <td><%= link_to "Show", accomplishment %></td>
      <td><%= link_to "Edit", edit_accomplishment_path(accomplishment) %></td>
      <td><%= link_to "Destroy", accomplishment, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Accomplishment", new_accomplishment_path %></p>
